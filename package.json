{
  "name": "eino-backend",
  "version": "1.0.0",
  "description": "Book and movie tracker with web and Android clients",
  "private": true,
  "main": "./dist/app.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "start": "npm run build && node ./dist/src/app.js",
    "dev": "nodemon",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "format": "prettier --write ./src",
    "test": "mocha --exit -r ts-node/register test/*.ts --timeout 10000",
    "migrate": "./node_modules/.bin/node-pg-migrate",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jankku/eino-backend.git"
  },
  "author": "Jankku",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "cachified": "^3.5.4",
    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.0.1",
    "luxon": "^3.4.3",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "swagger-ui-express": "^5.0.0",
    "tslib": "^2.6.2",
    "winston": "^3.10.0",
    "yaml": "^2.3.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.6",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.36",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/luxon": "^3.3.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.0",
    "@types/pg": "^8.10.2",
    "@types/pg-pool": "^2.0.3",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "chai": "^4.3.8",
    "chai-http": "^4.4.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "pin-github-action": "^1.8.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
