{
  "name": "eino-backend",
  "version": "1.0.0",
  "description": "Book and movie tracker with web and Android clients",
  "author": "Jankku",
  "license": "MIT",
  "private": true,
  "main": "./dist/app.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "start": "npm run build && node ./dist/src/app.js",
    "dev": "node -r ts-node/register --watch-path=src src/app.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "format": "prettier --write ./src",
    "migrate": "./node_modules/.bin/node-pg-migrate",
    "migrate:dev": "npx node-pg-migrate -m dist/migrations",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jankku/eino-backend.git"
  },
  "dependencies": {
    "@epic-web/cachified": "^5.2.0",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "@zxcvbn-ts/language-fi": "^3.0.2",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "canvas": "^3.0.0-rc2",
    "close-with-grace": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.2",
    "luxon": "^3.5.0",
    "mailtrap": "^3.4.0",
    "multer": "^1.4.5-lts.1",
    "node-pg-migrate": "^7.8.0",
    "otpauth": "^9.3.5",
    "pg-monitor": "^2.1.0",
    "pg-promise": "^11.10.2",
    "qrcode": "^1.5.4",
    "rfc4648": "^1.5.3",
    "sharp": "^0.33.5",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.8.1",
    "winston": "^3.17.0",
    "yaml": "^2.6.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-serve-static-core": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/luxon": "^3.4.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/qrcode": "^1.5.5",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "husky": "^9.1.7",
    "pin-github-action": "^1.9.1",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
