    "bcrypt": "^5.1.0",
    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^3.3.0",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.0",
    "pg-pool": "^3.6.0",
    "tslib": "^2.5.2",
    "winston": "^3.8.2",
    "zod": "^3.21.4"
