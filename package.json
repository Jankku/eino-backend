{
  "name": "eino-backend",
  "version": "1.0.0",
  "description": "Book and movie tracker with web and Android clients",
  "private": true,
  "main": "./dist/app.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "start": "npm run build && node ./dist/src/app.js",
    "dev": "nodemon",
    "lint": "eslint --ext .ts src/**/*",
    "lint:fix": "eslint --ext .ts --fix .src/**/*",
    "format": "prettier --write ./src",
    "test": "mocha --exit -r ts-node/register test/*.ts --timeout 10000",
    "migrate": "./node_modules/.bin/node-pg-migrate",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jankku/eino-backend.git"
  },
  "author": "Jankku",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cachified": "^3.5.4",
    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "lru-cache": "^10.0.0",
    "luxon": "^3.3.0",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.1",
    "pg-pool": "^3.6.1",
    "swagger-ui-express": "^4.6.2",
    "tslib": "^2.6.0",
    "winston": "^3.9.0",
    "yaml": "^2.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.5",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/luxon": "^3.3.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.0",
    "@types/pg": "^8.10.2",
    "@types/pg-pool": "^2.0.3",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "pin-github-action": "^1.8.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
