    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/luxon": "^3.3.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.0",
    "@types/pg": "^8.6.6",
    "@types/pg-pool": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
