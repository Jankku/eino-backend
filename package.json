{
  "name": "eino-backend",
  "version": "1.0.0",
  "description": "Book and movie tracker with web and Android clients",
  "author": "Jankku",
  "license": "MIT",
  "private": true,
  "main": "./dist/app.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "start": "npm run build && node ./dist/src/app.js",
    "dev": "node -r ts-node/register --watch-path=src src/app.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "format": "prettier --write ./src",
    "test": "mocha --exit -r ts-node/register test/*.ts --timeout 10000",
    "migrate": "./node_modules/.bin/node-pg-migrate",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jankku/eino-backend.git"
  },
  "dependencies": {
    "@epic-web/cachified": "^5.2.0",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "@zxcvbn-ts/language-fi": "^3.0.2",
    "axios": "^1.7.3",
    "bcrypt": "^5.1.1",
    "canvas": "^2.11.2",
    "close-with-grace": "^1.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.0",
    "luxon": "^3.5.0",
    "node-pg-migrate": "^7.6.1",
    "otpauth": "^9.2.2",
    "pg-monitor": "^2.1.0",
    "pg-promise": "^11.9.1",
    "qrcode": "^1.5.4",
    "rfc4648": "^1.5.3",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.6.3",
    "winston": "^3.14.1",
    "yaml": "^2.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@tsconfig/node18": "^18.2.4",
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^4.3.17",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/luxon": "^3.4.2",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.2.0",
    "@types/qrcode": "^1.5.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chai": "^5.1.1",
    "chai-http": "^4.4.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-unicorn": "^51.0.1",
    "husky": "^9.1.4",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.4",
    "pin-github-action": "^1.9.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
